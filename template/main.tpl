					function update_coupon(NoDo = false) {
						var i = 0;
						var coef;

						$.each($("betslip-bets").find("[data-id] .bet__coefficient"), function() {
								i++;
								if(i==1) {
									coef = $(this).text();
								} else {
									coef = coef * $(this).text();
								}
						});

						if(i >= 1 && NoDo == false) {
							$(".betslip").addClass("close");
						}

						if(i < 1) {
							$(".betslip").removeClass("close");
							$(".betslip").removeClass("open");
						}

						if(ChoseCouponType == 1) {
							$(".betslip-header-tabs #header-tab-single").addClass("selected");
							$(".betslip-header-tabs #header-tab-express").removeClass("selected");
							$(".betslip-footer-stake").css("display","none");
							$("betslip-bet[data-id] .bet__max-bet").css("display","block");
							$(".betslip-footer-total-list__name").text("Общая сумма ставки");
							$(".betslip-footer-total-list__value").text("0 RUB");
						} else {
							$(".betslip-header-tabs #header-tab-single").removeClass("selected");
							$(".betslip-header-tabs #header-tab-express").addClass("selected");
							$(".betslip-footer-stake").css("display","block");
							$("betslip-bet[data-id] .bet__max-bet").css("display","none");
							$(".betslip-footer-total-list__name").text("Общий коэффициент");
							$(".betslip-footer-total-list__value").text(parseFloat(coef).toFixed(2));
							$(".betslip-footer-possible-win__value").text("0 RUB");
						}

						storage.setItem('coupon', JSON.stringify($(".betslip").html()));

						$(".betslip-header-title-prev__title span").html(i);
						$(".betslip-header-title-text .betslip-counters").html(i);
						$(".betslip-header-title-prev__text span").html(parseFloat(coef).toFixed(2));
					};



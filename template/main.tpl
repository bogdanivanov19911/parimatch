			function showPopup(el) {
				if($('.popup').is('.active')) {
					$('.popup').removeClass('active');
				}
				if($(document).height() > $(window).height() && navigator.platform.indexOf('Mac') === -1) {
					$('body').addClass('active');
				}
				$('.overlay, .popup'+el).addClass('active');
			}

			function closePopup() {
				$('body, .overlay, .popup').removeClass('active');
			}


<div class="case-grid show" data-case-pricef="10" data-case-pricet="100" data-case-type="1">
    <div class="case">
        <a href="?do=cases&id={id}" class="mobile-url">Подробнее</a>
        <div class="hover eas">
            <div class="light"><a href="?do=cases&id={id}" class="btn-old orange rounded eas">Подробнее</a>
            </div>
        </div>
        <div class="price">
            <b>стоимость</b>{price} <span class="flaticon-ruble"></span>
        </div>
        <div class="img">
            <div class="prize"><img src="{img}" alt="Призовая обложка"></div>
            <div class="cover"><img src="/images/case/cases/case-cover.png" alt="Призовая обложка"></div>
        </div>
        <div class="payed">
            <div class="center">выдано {win}<span class="flaticon-ruble ruble-small"></span>
            </div>
        </div>
        <div class="case-footer"><a href="?do=cases&id={id}">Содержит от <b>{price_min}<span class="flaticon-ruble ruble-small"></span></b> до <b>{price_max}
                    <span class="flaticon-ruble ruble-small"></span></b></a>
        </div>
    </div>
</div>